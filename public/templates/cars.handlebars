<div id="home-page-results">
            <form class="form-horizontal search-form animated fadeInUp" />
            <div class="row">
                <div class="col-md-4">
                    <label class="control-label">Град</label>
                    <select class="form-control">
                    <option value="1">София</option>
                    <option value="3">Пловдив</option>
                    <option value="2">Бургас</option>
                </select>
                </div>
                <div class="col-md-4">
                    <label class="control-label">Марка</label>
                    <select class="form-control">
                        <option value="1">BMW</option>
                        <option value="3">Mercedes</option>
                        <option value="2">Trabant</option>
                    </select>
                </div>
                <div class="col-md-4">
                    <label class="control-label">Модел</label>
                    <select class="form-control">
                        <option value="1">156</option>
                        <option value="3">алфа</option>
                        <option value="2">кадет</option>
                    </select>
                </div>
            </div>
            <hr class="blabla" />
            <div class="row">
                <div class="col-md-4">
                    <label class="control-label">Година</label>
                    <select class="form-control">
                        <option value="1">Bus</option>
                        <option value="3">Car</option>
                        <option value="2">Caravan</option>
                    </select>
                </div>
                <div class="col-md-4">
                    <label class="control-label">Скоростна кутия</label>
                    <select class="form-control">
                        <option value="1">Bus</option>
                        <option value="3">Car</option>
                        <option value="2">Caravan</option>
                    </select>
                </div>
                <div class="col-md-4">
                    <label class="control-label">Двигател</label>
                    <select class="form-control">
                        <option value="1">Бензин</option>
                        <option value="3">Дизел</option>
                        <option value="2">Пропан</option>
                    </select>
                </div>
            </div>
            <hr />

            <div class="container">
                <h2>Резултати</h2>
                <div class="row">
                    {{#each cars}}
                    <div class="col-sm-4">
                        <div class="panel panel-default">
                            <div class="panel-heading text-center">BMW</div>
                            <div class="panel-body"><img src="images//no-image.png" class="img-responsive" style="width:100%" alt="Image"></div>
                            <div class="panel-footer">Buy 50 mobiles and get a gift card</div>
                        </div>
                    </div>
                     {{/each}}
                </div>
            </div><br>

        </div>



